            <div class="field-group col-sm-6">
                <label for="GuestDepartTime">Guest Depart Time</label>
                <input type="text" class="long" placeholder="Enter your departure time ..." value="" name="GuestDepartTime"/>
            </div>
        </div>

        <div class="row">
            <div class="field-group col-sm-12">
                <label for="BudgetPerPerson">Budget per person<span class="asterisk" style="top: 35px;">*</span></label>
                <input type="text" class="long" placeholder="Enter your budget ..." value="" name="BudgetPerPerson"/>
                <div class="error-msg hidden"></div>
            </div>
        </div>

        <div class="row">
            <div class="field-group col-xs-12">
                <label for="Comments">Comments</label>
                <textarea type="text" class="long" placeholder="Enter your comments..." value="" name="Comments"></textarea>
