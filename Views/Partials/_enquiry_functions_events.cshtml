            <div class="field-group col-sm-6">
                <label for="Company">Company</label>
                <input class="long" type="text" placeholder="Enter your company..." value="" name="Company">
            </div>
        </div>
        <div class="row">
            <div class="field-group col-sm-6">
                <label for="Phone">Phone <span class="asterisk" style="top: 35px;">*</span></label>
                <input type="text" class="long" placeholder="Enter your phone number..." value="" name="Phone"></input>
                <div class="error-msg hidden"></div>
            </div>
            <div class="field-group col-sm-6">
                <label for="Mobile">Mobile</label>
                <input type="text" class="long" placeholder="Enter your mobile number..." value="" name="Mobile"></input>
            </div>
        </div>
        <div class="row">
            <div class="field-group col-sm-6">
                <label for="Street">Street</label>
                <input type="text" class="long" placeholder="Enter your street..." value="" name="Street"></input>
            </div>
            <div class="field-group col-sm-6">
                <label for="Suburb">Suburb</label>
                <input type="text" class="long" placeholder="Enter your suburb..." value="" name="Suburb"></input>
            </div>
        </div>
        <div class="row">
            <div class="field-group col-sm-6">
                <label for="State">State</label>
                <select name="State" id="mce-MMERGE" class="cs-select">
                    <option value="Please select">Please select</option>
                    <option value="ACT">ACT</option>
                    <option value="NSW">NSW</option>
                    <option value="NT">NT</option>
                    <option value="QLD">QLD</option>
                    <option value="SA">SA</option>
                    <option value="TAS">TAS</option>
                    <option value="VIC">VIC</option>
                    <option value="WA">WA</option>
                </select>
            </div>
            <div class="field-group col-sm-6">
                <label for="Suburb">Postcode</label>
                <input type="text" class="long" placeholder="Enter your suburb..." value="" name="Postcode"/>
            </div>
        </div>

        <div class="row">
            <div class="field-group col-sm-6">
                <label for="ProposedEventDate">Proposed Event Date <span class="asterisk" style="top: 35px;">*</span></label>
                <input id="datepicker1" type="text" class="long" placeholder="Enter your proposed date ..." value="" name="ProposedEventDate"/>
                <div class="error-msg hidden"></div>
            </div>
            <div class="field-group col-sm-6">
                <label for="EventType">Event Type <span class="asterisk" style="top: 35px;">*</span></label>
                <select id="EventType" name="EventType" class="cs-select">
                    <option selected="selected" value="">Please select</option>
                    <option value="BoardRoom Luncheon">BoardRoom Luncheon</option>
                    <option value="Conference Catering">Conference Catering</option>
                    <option value="Corporate Breakfast">Corporate Breakfast</option>
                    <option value="Corporate Cocktail Party">Corporate Cocktail Party</option>
                    <option value="Corporate Dinner">Corporate Dinner</option>
                    <option value="Corporate Luncheon">Corporate Luncheon</option>
                    <option value="Funeral/Wake">Funeral/Wake</option>
