                <input type="number" class="long" placeholder="Enter a number..." value="" name="NumOfGuests"/>
                <div class="error-msg hidden"></div>
            </div>
        </div>

        <div class="row">
            <div class="field-group col-sm-6">
                <label for="GuestArrivalTime ">Guest Arrival Time</label>
                <input type="text" class="long" placeholder="Enter your arrival time ..." value="" name="GuestArrivalTime "/>
