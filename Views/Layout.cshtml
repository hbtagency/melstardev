            })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

            ga('create', 'UA-22849125-1', 'auto');
            ga('send', 'pageview');
        </script>
        <!--SmartPixel -->
        <script type="text/javascript">
            adroll_adv_id = "QPF7TKCCTRCGDPZDVULTP4";
