    <meta name="description" content="@Umbraco.GetDictionaryValue("Description")">
    <meta name="keywords" content="@Umbraco.GetDictionaryValue("Keywords")">
    <meta name="author" content="@Umbraco.GetDictionaryValue("Author")">

    <meta property="og:title" content="Melbourne Star" />
    <meta property="og:description" content="@Umbraco.GetDictionaryValue("Description")" />
    <meta property="og:url" content="https://www.facebook.com/melbournestarobservationwheel/" />
