@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = null;
}

<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="@Umbraco.GetDictionaryValue("Description")">
    <meta name="keywords" content="@Umbraco.GetDictionaryValue("Keywords")">
    <meta name="author" content="@Umbraco.GetDictionaryValue("Author")">

    <meta property="og:title" content="Melbourne Star" />
    <meta property="og:description" content="@Umbraco.GetDictionaryValue("Description")" />
    <meta property="og:url" content="https://www.facebook.com/melbournestarobservationwheel/" />
    <meta property="og:site_name" content="Melbourne Star" />

    <title> @Umbraco.Field("pageTitle") </title>
    <link rel="stylesheet" href="/css/final/style.css" />
    <link rel="shortcut icon" href="~/img/favicon.png" /

    <!-- JS init -->
    <script type="text/javascript" src="/js/jquery/jquery-2.1.4.min.js"></script>
    <script type="text/javascript" src="/js/bootstrap/bootstrap.min.js"></script>

    <!-- Type kit -->
    <script src="https://use.typekit.net/ety7fje.js"></script>
    <script>try { Typekit.load({ async: true }); } catch (e) { }</script>

    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.5.0-rc.1/angular.min.js"></script>
    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.5.0-rc.1/angular-animate.js"></script>

</head>
<body class="tk-proxima-nova">
    <header>
        <div id="body-mask" class="body-mask">
        </div>
        @Html.Partial("_search_mask")


        <!-- Load mobile nav which should only be displayed in Mobile and Tab view-->
        @Html.Partial("_mobile_nav")
        <!-- Load extra nav which hides in left side of box container. -->
        @Html.Partial("_extra_nav")

        @RenderBody()

        @Html.Partial("_footer")

        <!-- Instafeed -->
        <script type="text/javascript" src="/plugins/instafeed/instafeed2.min.js"></script>
        <script type="text/javascript" src="/js/project/instagram2.js"></script>

        <!-- Concanated JS -->
        <script type="text/javascript" src="/js/final/sitewide.js"></script>

        <!-- Google Anal-->
