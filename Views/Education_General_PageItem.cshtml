@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "~/Views/Layout.cshtml";
}

@Html.Partial("_header_bottom_nav")

<!-- template content area -->
<br /><br /><br/>

<main class="container transit template_content">

    <div class="row">
        <div class="col-lg-1 visible-lg">
        </div>
        <article class="col-lg-10 col-xs-12" id="table-striped-inside">
            @Html.Raw(CurrentPage.mainEditor)
        </article>
    </div>

    @if (CurrentPage.Children.Any())
    {
        <section class="row">
            <div class="col-lg-1 visible-lg">
            </div>
            <div class="col-lg-10 col-xs-12" id="table-striped-inside">
                <h1 style="margin-bottom:-50px">FAQ</h1>
                @Html.Partial("_FAQ")
            </div>
        </section>
    }
</main>

<script type="text/javascript" src="/js/final/striped_table.js"></script>


