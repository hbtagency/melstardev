@inherits Umbraco.Web.Mvc.UmbracoTemplatePage

@{
    Layout = "~/Views/Layout.cshtml";
}

@Html.Partial("_header")



<!-- template content area -->
<div class="container text-center clearfix">

    <div class="faq_container clearfix">
        <div class="accordion">
            <div class="accordion-group">
                <div class="accordion-heading collapsed_item">
                    <a class="accordion-toggle" data-toggle="collapse" data-parent="#leftMenu" href="#faq_sec1">
                        <span class="icon-list-alt">+</span> When is the last flight departure?
                    </a>
                </div>
                <div id="faq_sec1" class="accordion-body collapse" style="height: 0px; ">
                    <div class="accordion-inner">
                        <p>
                            Melbourne Star's last flight departs nightly at 9.30pm.  You must have purchased your ticket prior to 9.15pm
                            for the final flight. Please ensure you have had your ticket validated on Level 2 and made your way through
                            the Tales of Melbourne by 9.25pm to board the last flight.  Tickets can continue to be purchased for future dates until the Star closes at 10pm.
                            For more information on our hours of operation and ticket prices please visit Plan Your Visit.
                        </p>
                    </div>
                </div>
            </div>

            <div class="accordion-group">
